{
  "LambdaFunctionConfigurations": [
    {
      "Id": "documentgpt-processing-trigger",
      "LambdaFunctionArn": "arn:aws:lambda:us-east-1:995805900737:function:documentgpt-s3-trigger",
      "Events": ["s3:ObjectCreated:*"],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "prefix",
              "Value": "doc_"
            }
          ]
        }
      }
    }
  ]
}